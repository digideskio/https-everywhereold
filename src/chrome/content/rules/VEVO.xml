<ruleset name="VEVO">

	<target host="vevo.com" />
	<target host="*.vevo.com" />
	<target host="img.cache.vevo.com" />


	<rule from="^https?://(?:www\.)?vevo\.com/"
		to="https://www.vevo.com/" />

	<!--	cache, img.cache: Akamai
					-->
	<rule from="^https?://(?:img\.|s)?cache\.vevo\.com/"
		to="https://scache.vevo.com/" />

</ruleset>
