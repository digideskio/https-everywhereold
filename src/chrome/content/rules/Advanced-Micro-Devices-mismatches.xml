<ruleset name="Advanced Micro Devices (mismatches)" default_off="expired, mismatch">

	<!--	Akamai	-->
	<target host="developer.amd.com"/>
	<!--	Cert: *.ati.com		-->
	<target host="amd-member.com"/>
	<target host="www.amd-member.com"/>


	<rule from="^http://developer\.amd\.com/(_layouts/|afds/|Assets/|(?:Lists/Rotating|Publishing|SiteCollection)Images/|Style%20Library/|WebResource\.axd)"
		to="https://developer.amd.com/$1"/>

	<rule from="^http://(?:www\.)?amd-member\.com/"
		to="https://amd-member.com/"/>

</ruleset>
