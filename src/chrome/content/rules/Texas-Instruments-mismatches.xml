<!--
	For rules that are on by default, see Texas-Instruments.xml.

-->
<ruleset name="Texas Instruments (mismatches)" default_off="mismatch">

	<target host="ti.com" />
	<target host="www.ti.com" />


	<!--	Cookies are handled in the main ruleset.


		!www cert: www.ti.com
		www cert: Akamai
		!www redirects to www.	-->
	<rule from="^https?://(?:www\.)?ti\.com/"
		to="https://www.ti.com/" />

</ruleset>
