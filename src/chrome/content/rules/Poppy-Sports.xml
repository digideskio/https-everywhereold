<ruleset name="Poppy Sports">

	<target host="poppysports.com" />
	<target host="*.poppysports.com" />
	<target host="*.www.poppysports.com" />

	<rule from="^http://(?:www\.)?poppysports\.com/"
		to="https://www.poppysports.com/" />

	<securecookie host="^\.www\.poppysports\.com$" name=".*"/>

</ruleset>
