<!--
	For rulesets that are on by default, see Gentoo.xml.

-->
<ruleset name="Gentoo (mismatches)" default_off="mismatch">

	<target host="git.overlays.gentoo.org" />
	<target host="sidebar.gentoo.org" />


	<rule from="^http://(git\.overlays|sidebar)\.gentoo\.org/"
		to="https://$1.gentoo.org/" />

</ruleset>
