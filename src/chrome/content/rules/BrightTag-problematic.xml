<!--
	For rules that are on by default, see BrightTag.xml.

-->
<ruleset name="BrightTag (problematic)" default_off="mismatched">

	<target host="brighttag.com" />
	<target host="www.brighttag.com" />
	<target host="thebrightag.com" />
	<target host="www.thebrightag.com" />

	<!--	- CN: *.stage.thebrighttag.com
		- 301 like so
						-->
	<rule from="^https?://(?:www\.)?(?:the)?brighttag\.com/"
		to="https://www.brighttag.com/" />

</ruleset>