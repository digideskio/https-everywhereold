<!--
	For rules that are on by default, see Konera.xml.

-->
<ruleset name="Kontera (mismatches)" default_off="mismatch">

	<!--	Akamai	-->
	<target host="*.kontera.com" />


	<!--	Different buckets, but data appear the same.
								-->
	<rule from="^https?://(?:images|kona)\.kontera\.com/"
		to="https://kona.kontera.com/" />

</ruleset>
