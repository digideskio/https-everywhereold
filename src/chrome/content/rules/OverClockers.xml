<ruleset name="OverClockers.co.uk">
  <target host="overclockers.co.uk" />
	<!--	* for cross-domain cookies.	-->
  <target host="*.overclockers.co.uk" />

  <rule from="^http://(?:www\.)?overclockers\.co\.uk/" to="https://www.overclockers.co.uk/"/>

  <securecookie host="^.*\.overclockers\.co\.uk" name=".*"/>
</ruleset>
