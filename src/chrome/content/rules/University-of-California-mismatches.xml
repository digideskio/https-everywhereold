<ruleset name="University of California (mismatches)" default_off="mismatch">

	<target host="cnsi.ucla.edu"/>
	<target host="*.cnsi.ucla.edu"/>

	<rule from="^http://(?:www1?\.)?cnsi\.ucla\.edu/"
		to="https://www1.cnsi.ucla.edu/"/>

	<securecookie host="^www1\.cnsi\.ucla\.edu$" name=".*"/>

</ruleset>
