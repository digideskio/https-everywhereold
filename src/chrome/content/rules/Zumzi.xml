<!--
	Problematic domains:

		- img.zumzi.com *
		- s.zumzi.com *

	* CN: www.zumzi.com; shows that domain.

-->
<ruleset name="Zumzi (partial)" platform="mixedcontent">

	<target host="zumzi.ro" />
	<target host="*.zumzi.ro" />


	<securecookie host="^\.zumzi\.ro$" name=".+" />


	<rule from="^http://(www\.)?zumzi\.ro/"
		to="https://$1zumzi.ro/" />

</ruleset>