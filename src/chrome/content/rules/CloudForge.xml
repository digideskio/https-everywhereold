<ruleset name="CloudForge (partial)">

	<target host="*.cloudforge.com" />


	<securecookie host="^(?:ap|hel)p\.cloudforge\.com$" name=".+" />


	<rule from="^http://(ap|hel)p\.cloudforge\.com/"
		to="https://$1p.cloudforge.com/" />

</ruleset>