<!--
	Nonfunctional subdomains:

		- (www.)		(times out)
		- images		(ditto)
		- pages.newsletters	(ditto)

-->
<ruleset name="The National Academies Press (partial)">

	<target host="nap.edu" />
	<target host="*.nap.edu" />


	<!--	All appear the same.
					-->
	<rule from="^http://(?:cart\.|images\.|www\.)?nap\.edu/"
		to="https://cart.nap.edu/" />

</ruleset>
