<ruleset name="OpinionLab (partial)">

	<!--	* for cross-domain cookie.	-->
	<target host="*.opinionlab.com" />


	<securecookie host="^.*\.opinionlab\.com$" name=".*" />


	<rule from="^http://(oo|secure)\.opinionlab\.com/"
		to="https://$1.opinionlab.com/" />

</ruleset>
