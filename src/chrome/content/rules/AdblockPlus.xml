<ruleset name="AdblockPlus">

	<target host="adblockplus.org"/>
	<target host="*.adblockplus.org"/>


	<securecookie host="^(?:.*\.)?adblockplus\.org$" name=".+" />


	<rule from="^http://((?:easylist(?:-downloads)?|hg|reports|www)\.)?adblockplus\.org/"
		to="https://$1adblockplus.org/" />

</ruleset>
