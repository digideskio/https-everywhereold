<ruleset name="iHub_ Nairobi" default_off="mismatch">

	<!--	Cert: *.gridserver.com	-->
	<target host="ihub.co.ke" />
	<target host="*.ihub.co.ke" />
	<target host="www.research.ihub.co.ke" />


	<securecookie host="^ihub\.co\.ke$" name=".*" />


	<rule from="^https?://(?:www\.)?(research\.)?ihub\.co\.ke/"
		to="https://$1ihub.co.ke/" />

</ruleset>
