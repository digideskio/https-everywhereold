<!--
	Nonfunctional domains:

		- images.derstandard.at		(times out)

-->
<ruleset name="derStandard.at (partial)">

	<target host="derstandard.at" />
	<target host="*.derstandard.at" />


	<securecookie host="^\.derstandard\.at$" name=".*" />


	<rule from="^http://((?:android|blackberry|ipad|iphone|www)\.)?derstandard\.at/" 
		to="https://$1derstandard.at/" />

</ruleset>
