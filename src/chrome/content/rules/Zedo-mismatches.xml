<!--
	For rules that are on by default, see Zedo.xml.

-->
<ruleset name="Zedo (mismatches)" default_off="Akamai">

	<target host="*.zedo.com" />


	<securecookie host="^.*\.zedo\.com$" name=".+" />


	<!--	Tracking on 3rd-party websites.
						-->
	<rule from="^https?://d3\.zedo\.com/"
		to="https://d7.zedo.com/" />

	<rule from="^http://(d7|r1)\.zedo\.com/"
		to="https://$1.zedo.com/" />

</ruleset>
