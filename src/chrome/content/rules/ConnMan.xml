<ruleset name="ConnMan" default_off="expired, mismatch">

	<!--	cert: ofono.org		-->
	<target host="connman.net"/>
	<!--	* for cross-domain cookie	-->
	<target host="*.connman.net"/>

	<securecookie host="^\.connman\.net$" name=".*"/>

	<rule from="^http://(?:www\.)?connman\.net/"
		to="https://connman.net/"/>

</ruleset>
