<!--
	Related rulesets:

		- Buzzeff.xml


	Nonfunctional domains:

		- labs.ebuzzing.co.uk		(times out)
		- external.labs.ebuzzing.co.uk	(ditto)
		- (www.)ebuzzing.co.uk		(cert: *.ebuzzing.com; redirects to http)
		- (www.)ebuzzing.de		(ditto)
		- (www.)ebuzzing.es		(ditto)
		- (www.)ebuzzing.fr		(ditto)
		- (www.)ebuzzing.it		(ditto)
		- im.wk.io			(times out)
		- st0.wk.io			(cert: *.pantherssl.com; reset)

-->
<ruleset name="ebuzzing (partial)">

	<target host="ebuzzing.com" />
	<target host="www.ebuzzing.com" />


	<!--	- Prints domain name over https
		- Redirects to www over http
					-->
	<rule from="^https?://(?:www\.)?ebuzzing\.com/"
		to="https://ebuzzing.com/" />

</ruleset>
