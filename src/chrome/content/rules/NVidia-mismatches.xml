<ruleset name="nVidia (mismatches)" default_off="mismatch">

	<!--	Cert: speedway.plasticsmakeitpossible.com	-->
	<target host="blogs.nvidia.com" />
	<!--	Cert: acquia-sites.com	-->
	<target host="developer.nvidia.com" />
	<target host="forums.developer.nvidia.com" />


	<securecookie host="^(forums\.)?developer\.nvidia\.com$" name=".*" />


	<rule from="^http://(blogs|(?:forums\.)?developer)\.nvidia\.com/"
		to="https://$1.nvidia.com/" />

</ruleset>
