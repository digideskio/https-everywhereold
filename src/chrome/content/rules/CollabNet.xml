<!--
	Other CollabNet rulesets:

		- CloudForge.xml
		- Codesion.xml

-->
<ruleset name="CollabNet">

	<target host="collab.net" />
	<target host="*.collab.net" />


	<securecookie host="^\.collab\.net$" name=".+" />


	<rule from="^http://(blog\.|www\.)?collab\.net/"
		to="https://$1collab.net/" />

</ruleset>