<!--
	For rules that are on by default, see Ubisoft-Entertainment.xml.

-->
<ruleset name="Ubisoft Entertainment (mismatches)" default_off="mismatch">

	<!--	Cert: *.ubi.com		-->
	<target host="ubisoftgroup.com" />
	<target host="*.ubisoftgroup.com" />


	<!--	!www redirects to www.	-->
	<rule from="^http://(?:www\.)?ubisoftgroup\.com/"
		to="https://www.ubisoftgroup.com/" />

	<rule from="^http://common\.ubisoftgroup\.com/"
		to="https://common.ubisoftgroup.com/" />

</ruleset>
