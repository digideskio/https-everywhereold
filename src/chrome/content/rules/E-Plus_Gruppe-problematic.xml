<!--
	For rules that are on by default, see E-Plus_Gruppe.xml.

-->
<ruleset name="E-Plus Gruppe (problematic)" default_off="mismatched">

	<target host="eplus-gruppe.de" />


	<!--	wp-content/ does not redirect to !www,
		and so is handled in the main ruleset.
							-->
	<rule from="^http://eplus-gruppe\.de/(?!wp-content/)"
		to="https://eplus-gruppe.de/" />

</ruleset>