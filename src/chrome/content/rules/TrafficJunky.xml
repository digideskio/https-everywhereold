<ruleset name="TrafficJunky">

	<target host="trafficjunky.net" />
	<target host="*.trafficjunky.net" />


	<securecookie host="^www\.trafficjunky\.net$" name=".+" />


	<!--	ads: included on 3rd-party websites.
							-->
	<rule from="^http://(ads\.|www\.)?trafficjunky\.net/"
		to="https://$1trafficjunky.net/" />

</ruleset>
