<ruleset name="Dell">

	<target host="dell.com"/>
	<target host="linux.dell.com"/>
	<target host="www.dell.com"/>
	<target host="support.dell.com"/>

	<rule from="^http://(www\.)?dell\.com/"
		to="https://www.dell.com/"/>

	<rule from="^http://(linux|support)\.dell\.com/"
		to="https://$1.dell.com/"/>

</ruleset>

