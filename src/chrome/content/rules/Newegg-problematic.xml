<!--
	For rules that are on by default, see Newegg.xml.

-->
<ruleset name="Newegg (mismatches)" default_off="mismatch">

	<target host="images17.newegg.com" />
	<target host="promotions.newegg.com" />


	<!--	- Akamai
		- Not on ssl-images
					-->
	<rule from="^http://(images17|promotions)\.newegg\.com/"
		to="https://$1.newegg.com/" />

</ruleset>
