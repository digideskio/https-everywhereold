<!--
	For rules that are on by default, see Asana.xml.

-->
<ruleset name="Asana (mismatches)" default_off="mismatch">

	<!--	Cert: *.phpfogapp.com	-->
	<target host="asana.com" />
	<target host="www.asana.com" />
		<!--	Handled in main ruleset.	-->
		<exclusion pattern="^http://(www\.)?asana\.com/favicon\.ico" />


	<rule from="^https?://(?:www\.)?asana\.com/"
		to="https://asana.com/" />

</ruleset>
