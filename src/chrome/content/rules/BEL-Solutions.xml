<ruleset name="BEL Solution" default_off="self-signed">

	<!--	Cert: zeus.bel.fi	-->
	<target host="bel.fi" />
	<target host="*.bel.fi" />


	<!--	- Cert only matches zeus.bel.fi
		- All appear identical
			-->
	<rule from="^https?://(?:www\.|zeus\.)?bel\.fi/"
		to="https://zeus.bel.fi/" />

</ruleset>
