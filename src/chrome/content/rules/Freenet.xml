<ruleset name="Freenet">

	<target host="freenetproject.org" />
	<target host="*.freenetproject.org" />


	<securecookie host="^.*\.freenetproject\.org$" name=".*" />


	<rule from="^http://((?:bugs|checksums|downloads|emu|(?:new-|old-)?wiki|www)\.)?freenetproject\.org/"
		to="https://freenetproject.org/" />

</ruleset>
