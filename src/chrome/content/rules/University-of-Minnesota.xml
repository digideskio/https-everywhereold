<!--
	Nonfunctional subdomains:

		- www.cs		(times out)
		- glaros.dtc

-->
<ruleset name="University of Minnesota (partial)">

	<target host="shadow.cs.umn.edu" />
	<target host="hhh.umn.edu" />
	<target host="www.hhh.umn.edu" />


	<rule from="^http://(shadow\.cs|(?:www\.)?hhh)\.umn\.edu/"
		to="https://$1.umn.edu/" />

</ruleset>
