	<target host="enlightenment.org" />
	<target host="*.enlightenment.org" />


	<securecookie host="^(?:trac\.)?enlightenment\.org$" name=".*" />


	<rule from="^https?://(?:(trac\.)|www\.)?enlightenment\.org/"
		to="https://$1enlightenment.org/" />

</ruleset>
