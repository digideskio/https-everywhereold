<!--
	Nonfunctional domains:

		- docs	(shows ^)


	Problematic subdomains:

		- www	(400)

-->
<ruleset name="Crossrider (partial)">

	<target host="crossrider.com" />
	<target host="*.crossrider.com" />


	<securecookie host="^\.?crossrider\.com$" name=".+" />


	<rule from="^https?://(?:www\.)?crossrider\.com/"
		to="https://crossrider.com/" />

</ruleset>
