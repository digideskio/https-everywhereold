<!--
	Nonfunctional domains:

		- images.zap2it.com	(Akamai; 503)

-->
<ruleset name="Zap2it" default_off="mismatch">

	<target host="zap2it.com" />
	<target host="www.zap2it.com" />


	<!--	- !www times out
		- www: Akamai
				-->
	<rule from="^https?://(?:www\.)?zap2it\.com/"
		to="https://www.zap2it.com/" />

</ruleset>
