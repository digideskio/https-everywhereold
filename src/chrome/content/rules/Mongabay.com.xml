<!--
	For problematic rules, see Mongabay.com-problematic.xml.


	CDN buckets:

		- s3.amazonaws.com/mongabay-images/


	Nonfunctional subdomains:

		- (www.)	(cert: supporter.mongabay.com; 404)
		- photos	(ditto)

-->
<ruleset name="Mongabay.com (partial)">

	<target host="supporter.mongabay.com" />


	<securecookie host="^supporter\.mongabay\.com$" name=".+" />


	<rule from="^http://supporter\.mongabay\.com/"
		to="https://supporter.mongabay.com/" />

</ruleset>
