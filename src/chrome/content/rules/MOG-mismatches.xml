<!--
	For rules that are on by default, see MOG.xml.

-->
<ruleset name="MOG (mismatches)" default_off="mismatch">

	<!--	Cert: *.gridserver.com	-->
	<target host="blog.mog.com" />


	<securecookie host="^blog\.mog\.com$" name=".*" />


	<rule from="^http://blog\.mog\.com/"
		to="https://blog.mog.com/" />

</ruleset>
