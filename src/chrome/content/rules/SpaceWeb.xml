<!--
	vip-35.sweb.ru: self-signed


	Nonfunctional subdomains:

		- live	(times out)

-->
<ruleset name="SpaceWeb">

	<target host="sweb.ru"/>
	<target host="*.sweb.ru"/>


	<securecookie host="^\.sweb\.ru$" name=".*" />


	<rule from="^http://(cp\.|www\.)?sweb\.ru/"
		to="https://$1sweb.ru/" />

</ruleset>
