<ruleset name="Diaspora (mismatches)" default_off="mismatch">

	<target host="diasporaproject.org"/>
	<target host="www.diasporaproject.org"/>

	<securecookie host="^(.*\.)?diasporaproject\.org$" name=".*"/>

	<rule from="^http://(?:www\.)?diasporaproject\.org/"
		to="https://diasporaproject.org/"/>

</ruleset>
