<ruleset name="Centrum Wiskunde &amp; Informatica">

	<target host="cwi.nl" />
	<target host="*.cwi.nl" />


	<securecookie host="^\.cwi\.nl$" name=".*" />


	<!--	- !www doesn't work over https
		- !www redirects to www over http
			-->
	<rule from="^http://(?:www\.)?cwi\.nl/"
		to="https://www.cwi.nl/" />

</ruleset>
