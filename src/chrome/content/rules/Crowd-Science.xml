<!--
	support.crowdscience.com is handled in Zendesk.com-clients.xml.


	Nonfunctional subdomains:

		- (www.)
		- blog

-->
<ruleset name="Crowd Science (partial)">

	<target host="static.crowdscience.com" />
	<target host="secure-static.crowdscience.com" />


	<rule from="^http://app\.crowdscience\.com/"
		to="https://app.crowdscience.com/" />

	<!--	- Tracking on 3rd-party websites
		- static: Akamai
					-->
	<rule from="^https?://(?:secure-)?static\.crowdscience\.com/"
		to="https://secure-static.crowdscience.com/" />

</ruleset>
