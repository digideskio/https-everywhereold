<!--
	CDN buckets:

		- cdn.avanti247.com
			- cdn.putlocker.com


	At least some pages redirect to http.

-->
<ruleset name="PutLocker (partial)">

	<target host="putlocker.com" />
	<target host="*.putlocker.com" />


	<rule from="^http://images\.putlocker\.com/"
		to="https://images.putlocker.com/" />

	<rule from="^http://(www\.)?putlocker\.com/images/"
		to="https://$1putlocker.com/images/" />

</ruleset>