<ruleset name="Netverify">

	<target host="netverify.com" />
	<target host="*.netverify.com" />


	<securecookie host="^(?:pay\.)?netverify\.com$" name=".+" />


	<rule from="^http://(pay\.|static\.)?netverify\.com/"
		to="https://$1netverify.com/" />

</ruleset>