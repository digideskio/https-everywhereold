<!--	See Convio.xml also
-->
<ruleset name="Convio (mismatches)" default_off="Certificate mismatch">

	<target host="ir.convio.com"/>
	<target host="resources.convio.com"/>

	<rule from="^http://ir\.convio\.com/common/" to="https://ir.convio.com/common/"/>

	<rule from="^http://resources\.convio\.com/(cs|image)s/" to="https://resources.convio.com/$1s/"/>

	<securecookie host="^secure[23]\.convio\.net^" name=".*"/>

</ruleset>
