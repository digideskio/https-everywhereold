<ruleset name="Rapleaf">

	<target host="rapleaf.com" />
	<target host="*.rapleaf.com" />
	<target host="*.rlcdn.com" />


	<rule from="^http://(blog\.|dashboard\.|www\.)?rapleaf\.com/"
		to="https://$1rapleaf.com/" />

	<rule from="^http://(d|rc)\.rlcdn\.com/"
		to="https://$1.rlcdn.com/" />

</ruleset>
