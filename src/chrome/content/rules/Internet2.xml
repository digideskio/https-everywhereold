<!--
	Nonfunctional subdomains:

		- (www.)	(prints "https://www.internet2.edu/")

-->
<ruleset name="Internet2 (partial)">

	<target host="*.internet2.edu" />


	<securecookie host="^s(?:ervice1|paces)\.internet2\.edu$" name=".+" />


	<rule from="^http://s(ervice1|paces)\.internet2\.edu/"
		to="https://s$1.internet2.edu/" />

</ruleset>