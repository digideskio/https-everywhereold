<!--
	Nonfunctional subdomains:

		- account	(404)

-->
<ruleset name="Mouseflow (partial)">

	<target host="mouseflow.com" />
	<target host="*.mouseflow.com" />
		<!--
			404s(!)
				-->
		<exclusion pattern="^http://(?:www\.)?mouseflow\.com/sign-in" />


	<rule from="^http://(cdn\.|www\.)?mouseflow\.com/"
		to="https://$1mouseflow.com/" />

</ruleset>