<!--
	For rules that are on by default, see Mastermind.xml.

-->
<ruleset name="Mastermind (mismatches)" default_off="mismatch">

	<target host="mastermindpro.com" />
	<target host="www.mastermindpro.com" />


	<!--	- Cert: secure2.mastermindpro.com
		- At least some pages redirect from !www to www
					-->
	<rule from="^https?://(?:www\.)?mastermindpro\.com/"
		to="https://www.mastermindpro.com/" />

</ruleset>
