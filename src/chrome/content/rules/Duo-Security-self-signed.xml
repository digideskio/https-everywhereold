<!--
	For rules that are on by default, see Duo-Security.xml.

-->
<ruleset name="Duo Security (self-signed)" default_off="self-signed">

	<!--	Cert: www.duo	-->
	<target host="blog.duosecurity.com" />


	<rule from="^http://blog\.duosecurity\.com/"
		to="https://blog.duosecurity.com/" />

</ruleset>
