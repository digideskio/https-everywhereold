<ruleset name="Center for Constitutional Rights" default_off="expired, self-signed">

	<!--	Cert: plesk	-->
	<target host="ccrjustice.org" />
	<!--	* for cross-domain cookie.	-->
	<target host="*.ccrjustice.org" />


	<securecookie host="^\.ccrjustice\.org$" name=".*" />


	<rule from="^https?://(?:www\.)?ccrjustice\.org/"
		to="https://ccrjustice.org/" />

</ruleset>
