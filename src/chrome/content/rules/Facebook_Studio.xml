<ruleset name="Facebook Studio">

	<target host="facebook-studio.com" />
	<target host="*.facebook-studio.com" />


	<securecookie host="^(?:.+\.)?facebook-studio\.com$" name=".+" />


	<rule from="^http://(\w+\.)?facebook-studio\.com/"
		to="https://$1facebook-studio.com/" />

</ruleset>