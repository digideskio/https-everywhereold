<ruleset name="Cryptohaze">

	<target host="cryptohaze.com" />
	<target host="*.cryptohaze.com" />


	<securecookie host="^(?:.+\.)?cryptohaze\.com$" name=".+" />


	<rule from="^http://(webtables\.|www\.)?cryptohaze\.com/"
		to="https://$1cryptohaze.com/" />

</ruleset>