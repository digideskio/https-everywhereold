<!--
	See Collabora.xml for rules that are on by default.

-->
<ruleset name="Collabora (mismatches)" default_off="mismatch, self-signed">

	<target host="dhansak.collabora.co.uk"/>
	<target host="monkey.collabora.co.uk"/>
	<target host="trac.collabora.co.uk"/>

	<rule from="^http://(dhansak|monkey|trac)\.collabora\.co\.uk/"
		to="https://$1.collabora.co.uk/"/>

</ruleset>
